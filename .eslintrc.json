{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "extraFileExtensions": [".vue"],
    "sourceType": "module"
  },
  "plugins": ["vue"],
  "rules": {
    "vue/no-multiple-template-root": ["off"],
    "vue/multi-word-component-names": ["off"],
    "no-extra-parens": ["error"]
  }
}
